{
    "name": "Tiktok AF",
    "description": "Just open Tiktok and this extension will take care of the rest, auto-scrolling you for hours!",
    "version": "1.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.tiktok.com/*"],
            "css": ["main.css"],
            "js": ["main.js"]
        }
    ],
    "icons": {
        "16": "img/AF16.png",
        "48": "img/AF48.png",
        "128": "img/AF128.png"
    }
}
