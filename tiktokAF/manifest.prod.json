{
    "name": "Tiktok AF",
    "description": "Tiktok auto-scroll, as well as faster play (1.5 regular speed)",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.tiktok.com/*"],
            "css": ["main.css"],
            "js": ["main.js"]
        }
    ]
}



