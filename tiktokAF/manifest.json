{
    "name": "Tiktok AF",
    "description": "Tiktok autoplay, as well as fast play (1.5 regular speed)",
    "version": "1.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://www.tiktok.com/*"],
            "css": ["app.css"],
            "js": ["app.js"]
        }
    ]
}
